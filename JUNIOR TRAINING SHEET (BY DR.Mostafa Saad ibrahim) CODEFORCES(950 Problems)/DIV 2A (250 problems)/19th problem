



https://codeforces.com/contest/228/problem/A   ---PROBLEM LINK 









#include<bits/stdc++.h>
using namespace std;
#define ll long long int
int main()
{
   ios_base::sync_with_stdio(false);
   cin.tie(NULL);
   cout.tie(NULL);
   int size=4;
   unordered_set<int>s;
   for(int i=0;i<4;i++)
   {
      ll x;
      cin>>x;
      s.insert(x);
   }
   int diff=size-s.size();
   cout<<diff<<endl;
}
