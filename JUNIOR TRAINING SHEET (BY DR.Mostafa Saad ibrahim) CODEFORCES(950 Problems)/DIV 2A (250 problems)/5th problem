

https://codeforces.com/contest/263/problem/A   --PROBLEM LINK






#include<bits/stdc++.h>
using namespace std;
#define ll long long int
int main()
{
  ios_base::sync_with_stdio(false);
  cin.tie(NULL);
  cout.tie(NULL);
  int row,column;
  bool ans=false;
  for(int i=1;i<=5;i++)
  {
    for(int j=1;j<=5;j++)
    {
       ll x;
       cin>>x;
       if(x==1)
       {
          row=i;column=j;
          ans=true;
          break;
       }
    }
    if(ans)
    break;
    
  }

  int ans2=abs(3-row)+abs(3-column);
  cout<<ans2<<endl;
}
