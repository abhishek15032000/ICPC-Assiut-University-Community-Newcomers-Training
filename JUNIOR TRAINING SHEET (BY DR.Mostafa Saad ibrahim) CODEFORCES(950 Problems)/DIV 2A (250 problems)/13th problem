https://codeforces.com/contest/266/problem/A   ---PROBLEM LINK




#include<bits/stdc++.h>
using namespace std;
#define ll long long int
int main()
{
   ios_base::sync_with_stdio(false);
   cin.tie(NULL);
   cout.tie(NULL);
   ll n;
   cin>>n;
   cin.get();
   string s;
   getline(cin,s);
   list<char>l;
   for(int i=0;i<s.length();i++)
   {
      l.push_back(s[i]);
   }
   l.unique();
   int diff=s.size()-l.size();
   cout<<diff<<endl;
}
