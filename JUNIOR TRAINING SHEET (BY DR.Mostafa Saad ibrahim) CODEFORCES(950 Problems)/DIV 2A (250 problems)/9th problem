https://codeforces.com/contest/59/problem/A  ---PROBLEM LINK 





#include<bits/stdc++.h>
using namespace std;
#define ll long long int
int main()
{
   ios_base::sync_with_stdio(false);
   cin.tie(NULL);
   cout.tie(NULL);
   int count_Lower=0;
   int count_upper=0;
   string s;
   getline(cin,s);
   for(int i=0;i<s.length();i++)
   {
      if(s[i]>='a' and s[i]<='z')
      {
        count_Lower++;
      }
      else
      {
        count_upper++;
      }
   }
   if(count_Lower>=count_upper)
   {
     transform(s.begin(),s.end(),s.begin(),::tolower);
     cout<<s<<endl;
   }
   else
   {
       transform(s.begin(),s.end(),s.begin(),::toupper);
       cout<<s<<endl;
   }
}
