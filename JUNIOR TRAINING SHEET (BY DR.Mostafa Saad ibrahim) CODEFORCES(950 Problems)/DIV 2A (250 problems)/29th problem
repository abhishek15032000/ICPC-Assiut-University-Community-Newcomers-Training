#include<bits/stdc++.h>
using namespace std;
#define ll long long int
int main()
{
  ios_base::sync_with_stdio(false);
  cin.tie(NULL);
  cout.tie(NULL);
  ll size=1e5+1;
  bool arr[size]={0};
  ll n;
  cin>>n;
  ll target=n;
  ll x;
  for(int i=1;i<=n;i++)
  {
    cin>>x;
    arr[x]=1;
    if(x==target)
    {
       for(int j=x;j>=1;j--)
       {
             if(!visited[j])
             break;
             cout<<j;
             target=j-1;
        }
    }
    cout<<endl;
    }
 }
