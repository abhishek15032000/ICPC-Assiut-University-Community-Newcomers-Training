https://codeforces.com/contest/768/problem/A   ---PROBLEM LINK 







#include<bits/stdc++.h>
using namespace std;
#define ll long long int
int main()
{
  ios_base::sync_with_stdio(false);
  cin.tie(NULL);
  cout.tie(NULL);
  ll n;
  cin>>n;
  ll arr[n];
  for(int i=0;i<n;i++)
  {
     cin>>arr[i];
  }
  ll maxi=*max_element(arr,arr+n);
  ll mini=*min_element(arr,arr+n);
  ll count=0;
  for(int i=0;i<n;i++)
  {
     if(arr[i]>mini and arr[i]<maxi)
     {
        count++;
     }
  }
  cout<<count<<endl;
}
