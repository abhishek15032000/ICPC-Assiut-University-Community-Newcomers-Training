


https://codeforces.com/contest/443/problem/A   ---PROBLEM LINK 








#include<bits/stdc++.h>
using namespace std;
#define ll long long int
int main()
{
  ios_base::sync_with_stdio(false);
  cin.tie(NULL);
  cout.tie(NULL);
  string s;
  getline(cin,s);
  unordered_set<char>st;
  for(int i=0;i<s.length();i++)
  {
     if(s[i]>='a' and s[i]<='z')
     {
        st.insert(s[i]);
     }
  }
  cout<<st.size()<<endl;
}
