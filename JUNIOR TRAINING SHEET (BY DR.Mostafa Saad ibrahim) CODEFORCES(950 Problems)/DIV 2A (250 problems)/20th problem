
http://codeforces.com/contest/9/problem/A   ---PROBLEM LINK 







#include<bits/stdc++.h>
using namespace std;
#define ll long long int
int main()
{
   ios_base::sync_with_stdio(false);
   cin.tie(NULL);
   cout.tie(NULL);
   ll a,b;
   cin>>a>>b;
   int maxi=max(a,b);
   int favourable_cases=6-maxi;
   if(favourable_cases==1)
   { //5 and 6
     cout<<1<<"/"<<3<<endl;
   }
   if(favourable_cases==2)
   {
     //4 ,5 and 6
     cout<<1<<"/"<<2<<endl;
   }
   if(favourable_cases==3)
   {
     //3 4 5 6
     cout<<2<<"/"<<3<<endl;
   }
   if(favourable_cases==4)
   {
      //2 3 4 5 6
      cout<<5<<"/"<<6<<endl;
   }

   if(favourable_cases==5)
   {
     //1 2 3 4 5 6
     cout<<1<<"/"<<1<<endl;
   }

   if(favourable_cases==0)
   {
     cout<<1<<"/"<<6<<endl;
   }
}
