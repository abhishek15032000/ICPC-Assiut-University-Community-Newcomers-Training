

http://codeforces.com/contest/791/problem/A  --PROBLEM LINK


#include<bits/stdc++.h>
using namespace std;
#define ll long long int
int main()
{
   ios_base::sync_with_stdio(false);
   cin.tie(NULL);
   cout.tie(NULL);
   ll a,b;
   cin>>a>>b;
   int i=0;
   while(a<=b)
   {
      i++;
      a=a*3;
      b=b*2;
   }
   cout<<i<<endl;
}
