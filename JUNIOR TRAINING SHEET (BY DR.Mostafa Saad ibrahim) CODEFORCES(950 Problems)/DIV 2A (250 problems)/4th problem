
https://codeforces.com/contest/231/problem/A   --PROBLEM LINK 




#include<bits/stdc++.h>
using namespace std;
#define ll long long int 
int main()
{
  ios_base::sync_with_stdio(false);
  cin.tie(0);
  cout.tie(0);
  ll n;
  cin>>n;
  ll ans=0;
  for(int i=0;i<n;i++)
  {  int count=0;
     for(int i=0;i<3;i++)
     { 
        ll x;
        cin>>x;
        if(x==1)
        count++;
     }
     if(count>=2)
     {
        ans++;
     }
  }
  cout<<ans<<endl;
} 
