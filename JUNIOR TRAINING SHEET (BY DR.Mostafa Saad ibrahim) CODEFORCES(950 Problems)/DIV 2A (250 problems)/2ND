
https://codeforces.com/contest/734/problem/A  --problem link 








#include<bits/stdc++.h>
using namespace std;
#define ll long long int
int main()
{
   ios_base::sync_with_stdio(false);
   cin.tie(NULL);
   cout.tie(NULL);
   ll n;
   cin>>n;
   cin.get();
   string s;
   getline(cin,s);
   unordered_map<char,int>m;
   for(int i=0;i<s.length();i++)
   {
      m[s[i]]++;
   }
   if(m['A']>m['D'])
   {
      cout<<"Anton"<<endl;
   }
   if(m['A']<m['D'])
   {
      cout<<"Danik"<<endl;
   }
   if(m['A']==m['D'])
   {
      cout<<"Friendship"<<endl;
   }
}
