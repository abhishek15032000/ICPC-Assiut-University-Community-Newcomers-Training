


https://codeforces.com/group/MWSDmqGsZm/contest/223206/problem/A   - -----PROBLEM LINK















#include<bits/stdc++.h>
using namespace std;
#define ll long long int 
int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    cout.tie(NULL);
    string str;
    getline(cin,str);
    int i=0;
    int j=str.length()-1;
    if(str.length()==1)
    {
        if(str[0]=='?')
        {
            str[0]='a';
        }
        cout<<str<<endl;
        return 0;
    }
    bool ans1=true;
    while(i<=j)
    {
        if(str[i]!='?' and str[j]!='?')
        {
            if(str[i]!=str[j])
            {   ans1=false;
                break;
            }
            else
            {
                i++;
                j--;
            }
        }
        else if(str[i]!='?' and str[j]=='?')
        {
            str[j]=str[i];
            j--;
            i++;
        }
        else if(str[i]=='?' and str[j]!='?')
        {
            str[i]=str[j];
            i++;j--;
        }
        else if(str[i]=='?' and str[j]=='?')
        {
            str[i]='a';
            str[j]='a';
            i++;
            j--;
        }

    }
    if(ans1)
    {
        cout<<str<<endl;
    }
    else
    {
        cout<<-1<<endl;
    }
}
