
https://codeforces.com/group/MWSDmqGsZm/contest/223206/problem/F   -----PROBLEM LINK













#include<bits/stdc++.h>
using namespace std;
#define ll long long int 
int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    cout.tie(NULL);
    ll n,m;
    cin>>n>>m;
    bool flag=true;
    for(int i=1;i<=n;i++)
    {
        if(i&1)
        {
            for(int j=1;j<=m;j++)
            {
                cout<<"#";
            }
            cout<<"\n";
        }
        else
        {
            if(flag)
            {
                for(int j=1;j<=m-1;j++)
                {
                    cout<<".";
                }
                cout<<"#"<<"\n";
                flag=false;
            }
            else
            {   cout<<"#";
                for(int j=1;j<=m-1;j++)
                {
                    cout<<".";
                }
                cout<<"\n";
                flag=true;
            }
        }
    }
}
