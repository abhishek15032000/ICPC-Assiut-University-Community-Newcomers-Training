 
#include<bits/stdc++.h>
using namespace std;
#define mod 100000000007
#define ll long long int 
int main()
{   ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    cout.tie(NULL);
    ll a,b,c,d;
    cin>>a>>b>>c>>d;
    ll sum=(((a%mod)*(b%mod))%mod)-(((c%mod)*(d%mod))%mod);
    cout<<"Difference = "<<sum<<endl;
}
