#include<bits/stdc++.h>
using namespace std;
#define ll long long int
int main()
{
   ios_base::sync_with_stdio(false);
   cin.tie(NULL);
   cout.tie(NULL);
   ll n,m;
   cin>>n>>m;
   switch(m)
   {
      case 2:{
          string temp;
          temp.push_back('2');
          for(int i=1;i<=n-1;i++)
          {
             int x=1+rand()%9;
             char y=x+'0';
             temp=y+temp;
          }
          cout<<temp<<endl;
          break;
      }

      case 3:
      {
          string temp;
          temp.push_back('3');
          for(int i=1;i<=n-1;i++)
          {
             char x=3+'0';
             temp=x+temp;
          }
          cout<<temp<<endl;
          break;
      }


      case 4:
      {
           
             string temp;
          
          for(int i=1;i<=n;i++)
          {
             temp.push_back('4');
          }
          cout<<temp<<endl;
          break;
      }

      case 5:
      {
         string temp;
         temp.push_back('5');
         for(int i=1;i<=n-1;i++)
         {
            int x=1+rand()%9;
            char y=x+'0';
            temp=y+temp;
         }
         cout<<temp<<endl;
         break;
      }

      case 6:
      {
          string temp;
          for(int i=1;i<=n;i++)
          {
            temp.push_back('6');
          }
          cout<<temp<<endl;
          break;
      }
      case 7:
      {
          
       string temp;
       for(int i=1;i<=n;i++)
       {
         temp.push_back('7');
       }
       cout<<temp<<endl;
       break;

      }
      case 8:
      {    string temp;
           
           
           for(int i=1;i<=n;i++)
           {
             temp.push_back('8');
           }
           cout<<temp<<endl;
           break;
      }

      case 9:
      {  string temp;
         for(int i=1;i<=n;i++)
         {
            temp.push_back('9');

         }
         cout<<temp<<endl;
         break;
      }

      case 10:{
        string temp;
         temp.push_back('0');
         if(n<=1)
         {
            cout<<-1<<endl;
         }
         else
         {for(int i=1;i<=n-1;i++)
         {
            int x=1+rand()%9;
            char y=x+'0';
            temp=y+temp;
         }
         cout<<temp<<endl;
         }
         break;
      }
   }
}
