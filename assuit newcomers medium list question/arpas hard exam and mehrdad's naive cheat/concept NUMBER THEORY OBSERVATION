https://codeforces.com/group/MWSDmqGsZm/contest/223207/problem/B ---PROBLEM LINK









#include<bits/stdc++.h>
using namespace std;
#define ll long long int 
int main()
{
   ios_base::sync_with_stdio(false);
   cin.tie(NULL);
   cout.tie(NULL);
   ll n;
   cin>>n;
   if(n==1)
   {
     cout<<8<<endl;
   }
   else if(n==0)
   {
     cout<<1<<endl;
   }
   else
   {
     if(n%4==1)
     {
       cout<<8<<endl;
     }
     else if(n%4==2)
     {
        cout<<4<<endl;
     }
     else if(n%4==3)
     {
       cout<<2<<endl;
     }
     else if(n%4==0)
     {
        cout<<6<<endl;
     }
   }
}
