
https://codeforces.com/group/MWSDmqGsZm/contest/223207/problem/L  ---PROBLEM LINK








#include<bits/stdc++.h>
using namespace std;
#define ll long long int
int main()
{
   ios_base::sync_with_stdio(false);
   cin.tie(NULL);
   cout.tie(NULL);
   map<int,int>m;
   ll n;
   cin>>n;
   for(int i=0;i<n;i++)
   {
      ll x;
      cin>>x;
      m[x]++;
   }
   bool ans=true;
   int count=0;
   for(auto it=m.begin();it!=m.end();it++)
   {
      if(it->second>1)
      {ans=false;
       count+=(it->second-1);
      }
   }

   if(ans)
   {
     cout<<-1<<endl;
   }
   else
   {
      cout<<(count)<<endl;
   }
}
