#include <bits/stdc++.h>
using namespace std;
#define ll long long int 
int main()
{   ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    cout.tie(NULL);
	ll a,b,c,d;
	cin>>a>>b>>c>>d;
    long double x=b*log10(a);
    long double y=d*log10(c);
    if(x>y)
    {
    	cout<<"YES"<<"\n";
    }
    else
    {
    	cout<<"NO"<<"\n";
    }
	
}
