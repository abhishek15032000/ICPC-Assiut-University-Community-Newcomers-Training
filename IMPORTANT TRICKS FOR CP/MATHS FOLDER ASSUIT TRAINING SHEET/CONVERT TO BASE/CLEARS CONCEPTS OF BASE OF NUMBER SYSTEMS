CODE//




#include<bits/stdc++.h>
using namespace std;
#define ll long long int
ll binary_expo(ll base,ll power)
{   ll res=1;
    while(power)
    {
        if(power&1)
        {res=res*base;
        power--;
        }
        base=base*base;
        power>>=1;
    }
    return res;
}
int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(0);
    cout.tie(0);
    ll t;
    cin>>t;
    int j=10;
     map<char,ll>m;
    for(int i=0;i<26;i++)
    {
        m[i+65]=j;
         j++;
    }
   /* for(auto it=m.begin();it!=m.end();it++)
    {
        cout<<it->first<<" "<<it->second<<endl;
    }*/
    if(t==1)
    {   cin.get();
        string s;ll x;
        cin>>s>>x;
       ll res=0;
       ll j=0;
       for(ll i=s.length()-1;i>=0;i--)
       {   if(s[i]>='A' and s[i]<='Z')
           {
               res+=(m[s[i]]*binary_expo(x,j));
           }
           else
            {
                res+=(s[i]-'0')*binary_expo(x,j);
            }
            j++;
      }
      cout<<res<<endl;
    }
    if(t==2)
    {
       ll n,x;
       cin>>n>>x;
       string res;
       while(n)
       {
           ll r=n%x;
           //cout<<r<<endl;
           if(r>=10 and r<=35)
           {   
               for(auto it=m.begin();it!=m.end();it++)
               {
                   if(it->second==r)
                   {   //cout<<it->first<<endl;
                       res+=it->first;
                       break;
                   }
               }
           }
           else
           {  // cout<<r<<endl;
               res+=to_string(r);
           }
           n=n/x;
       }
      for(int i=res.length()-1;i>=0;i--)
      {
          cout<<res[i];
      }
    }
}
