https://codeforces.com/group/MWSDmqGsZm/contest/223338/problem/X   ----PROBLEM LINK




CODE/////



#include<bits/stdc++.h>
using namespace std;
#define ll long long int
int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    cout.tie(NULL);
    ll x1,y1,x2,y2,x3,y3,x4,y4;
    cin>>x1>>y1>>x2>>y2>>x3>>y3>>x4>>y4;

    ll highest_x=max(max({x1,x2,x3}),x4);
    ll highest_y=max(max({y1,y2,y3}),y4);
    ll lowest_x=min(min({x1,x2,x3}),x4);
    ll lowest_y=min(min({y1,y2,y3}),y4);

    ll n;
    cin>>n;
    while(n--)
    {
        ll r1,r2;
        cin>>r1>>r2;

        if((r1>=lowest_x and r1<=highest_x) and (r2>=lowest_y and r2<=highest_y))
        {
            cout<<"YES"<<"\n";
        }
        else
        {
            cout<<"NO"<<"\n";
        }
    }

}
