#include<bits/stdc++.h>
using namespace std;
#define ll long long int
int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    cout.tie(NULL);
    ll a,b,q;
    cin>>a>>b>>q;
    if(q==1)
    cout<<a<<endl;
    else if(q==2)
    cout<<b<<endl;
    else
   {  if(q%3==0)
      {
          cout<<(a^b)<<endl;
      }
      else if(((q+1)%3)==0)
      {
          cout<<b<<endl;
      }
      else{
          cout<<a<<endl;
      }
   
   }
}
