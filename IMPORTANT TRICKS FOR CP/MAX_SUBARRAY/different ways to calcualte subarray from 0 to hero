THE ABOVE QUESTIONS SOLUTION:-


#include<bits/stdc++.h>
using namespace std;
#define ll long long int
int main()
{
	ios_base::sync_with_stdio(false);
	cin.tie(NULL);
	cout.tie(NULL);
	ll t;
	cin>>t;
	while(t--)
   {
   	ll n;
	cin>>n;
	ll arr[n];
	for(ll i=0;i<n;i++)
	{
		cin>>arr[i];
	}
	vector<ll> temp;
	for(ll i=0;i<n;i++)
	{
		for(ll j=i;j<n;j++)
		{   	vector<ll> temp2;
			for(ll k=i;k<=j;k++)
			{
			   temp2.push_back(arr[k]);
			}
			ll max=*max_element(temp2.begin(),temp2.end());
			temp.push_back(max);
		}
	}
	for(auto &x:temp)
	{
		cout<<x<<" ";
	}
	cout<<endl;
   }
}
