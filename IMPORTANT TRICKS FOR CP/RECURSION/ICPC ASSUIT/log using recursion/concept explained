

https://codeforces.com/group/MWSDmqGsZm/contest/223339/problem/P   ---PROBLEM LINK








#include<bits/stdc++.h>
using namespace std;
#define ll long long int

ll print(ll n)
{   if(n==1)
    return 0;
    return 1+print(n/2);
}
int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    cout.tie(NULL);
    ll n;
    cin>>n;
    cout<<print(n)<<endl;
}
