REFERENCE -https://codeforces.com/blog/entry/67890   ----HIS QUESTION ON CODEFORCES 







#include<bits/stdc++.h>
using namespace std;
#define ll long long int
int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    cout.tie(NULL);
    ll n,r;
    cin>>n>>r;
    ll ans=1;
    for(ll i=1;i<=r;i++){
        ans=ans*(n-i+1)/i;
    }
    cout<<ans<<endl;
}
