


unsigned long long int expo(ll a,ll b,ll n)
{
    ll ans=1;
    while(b)
    {
        if(b&1)
        {
            ans=(ans*a)%n;
            b--;
        }
        a=(a*a)%n;
        b>>=1;
    }
    return ans;
}
bool fermats_test(ll n,ll iterations=10)
{
    if(n<=1 or n==4)
    return false;
    if(n<=3)
    return true;
    for(int i=0;i<iterations;i++)
    {
        ll a=2+rand()%(n-4);
        if(__gcd(a,n)!=1)
        return false;
        if(expo(a,n-1,n)!=1)
        return false;
    }
    return true;
}
