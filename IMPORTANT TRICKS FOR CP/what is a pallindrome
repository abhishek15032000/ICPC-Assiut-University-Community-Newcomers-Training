https://codeforces.com/group/MWSDmqGsZm/contest/219856/problem/I    ----  pallindrome problem 





#include<bits/stdc++.h>
using namespace std;
#define ll long long int
int main()
{
	ios_base::sync_with_stdio(false);
	cin.tie(NULL);
	cout.tie(NULL);
	string temp;
	getline(cin,temp);
	int i=0;
	int j=temp.length()-1;
	while(i<j)
	{
		if(temp[i]==temp[j])
		{i++;j--;}
		else
		{cout<<"NO"<<endl;
		return 0;}
	}
	cout<<"YES"<<endl;
}
